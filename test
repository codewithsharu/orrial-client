% Professional color scheme
\definecolor{primary}{HTML}{000000}    % Black
\definecolor{secondary}{HTML}{333333}   % Dark gray
\definecolor{accent}{HTML}{666666}      % Medium gray
\definecolor{text}{HTML}{000000}        % Black text
\definecolor{lighttext}{HTML}{444444}   % Dark gray text
\definecolor{background}{HTML}{FFFFFF}  % White background

\colorlet{name}{primary}
\colorlet{tagline}{secondary}
\colorlet{heading}{primary}
\colorlet{headingrule}{secondary}
\colorlet{accent}{accent}
\colorlet{emphasis}{text}
\colorlet{body}{lighttext}

% Professional font
\usepackage[default]{sourcesanspro}
\usepackage[T1]{fontenc}

