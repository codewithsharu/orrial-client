% Professional section formatting
\titleformat{\section}{
  \vspace{-4pt}\scshape\raggedright\large\bfseries\color{heading}
}{}{0em}{}[\color{headingrule}\titlerule \vspace{-5pt}]

% Ensure PDF is machine readable
\pdfgentounicode=1

% Professional custom commands
\newcommand{\resumeItem}[1]{
  \item\small{
    {#1 \vspace{-2pt}}
  }
}

\newcommand{\resumeSubheading}[4]{
  \vspace{-2pt}\item
    \begin{tabular*}{1.0\textwidth}[t]{l@{\extracolsep{\fill}}r}
      \textbf{\large\color{primary}#1} & \textbf{\small\color{secondary} #2} \\
      \textit{\large\color{lighttext}#3} & \textit{\small\color{lighttext} #4} \\
    \end{tabular*}\vspace{-7pt}
}

\newcommand{\resumeProjectHeading}[2]{
    \item
    \begin{tabular*}{1.001\textwidth}{l@{\extracolsep{\fill}}r}
      \small\color{primary}#1 & \textbf{\small\color{secondary} #2}\\
    \end{tabular*}\vspace{-7pt}
}

